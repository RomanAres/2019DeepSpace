{
  "java.configuration.updateBuildConfiguration": "automatic",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    ".classpath": true,
    ".project": true
  },
  "wpilib.online": true,
  "java.test.config": [
    {
      "name": "includeNativesConfiguration",
      "workingDirectory": "${workspaceFolder}",
      "args": [],
      "vmargs": [],
      "env": { "PATH":"build/tmp/jniExtractDir"}
    }
  ],
  "java.test.defaultConfig": "includeNativesConfiguration"
}
